import java.io.*;

public class JavaApplication5{
static int findNonRepeating(int []arr,  
                            int n) 
{ 
    int res = 0; 
    for (int i = 0; i < n; i++) 
        res = res ^ arr[i]; 
    return res; 
} 
    /**
     *
     * @param args
     */
    public static void main(String args[])
{ 
    int arr[] = { 3, 8, 3, 2, 2, 1, 1, 6, 6 }; 
    int n = arr.length; 
    System.out.println(findNonRepeating(arr, n));  
}
}
